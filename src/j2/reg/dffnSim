#! /opt/homebrew/Cellar/icarus-verilog/12.0/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/system.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_sys.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/vhdl_textio.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/v2005_math.vpi";
:vpi_module "/opt/homebrew/Cellar/icarus-verilog/12.0/lib/ivl/va_math.vpi";
S_0x11ef05860 .scope module, "dffnSim" "dffnSim" 2 1;
 .timescale 0 0;
v0x11ef1ae30_0 .net "ck", 0 0, v0x11ef059d0_0;  1 drivers
v0x11ef1af10_0 .var "i", 1 0;
v0x11ef1afa0_0 .net "o", 1 0, v0x11ef1ad00_0;  1 drivers
S_0x11ef06c90 .scope module, "clk1" "clk" 2 4, 3 1 0, S_0x11ef05860;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "ck";
v0x11ef059d0_0 .var "ck", 0 0;
S_0x11ef1a8a0 .scope module, "dffn1" "dffn" 2 5, 4 1 0, S_0x11ef05860;
 .timescale 0 0;
    .port_info 0 /OUTPUT 2 "Q";
    .port_info 1 /INPUT 2 "D";
    .port_info 2 /INPUT 1 "ck";
P_0x11ef1aa60 .param/l "N" 0 4 4, +C4<00000000000000000000000000000010>;
v0x11ef1ac40_0 .net "D", 1 0, v0x11ef1af10_0;  1 drivers
v0x11ef1ad00_0 .var "Q", 1 0;
v0x11ef1ada0_0 .net "ck", 0 0, v0x11ef059d0_0;  alias, 1 drivers
E_0x11ef1abf0 .event negedge, v0x11ef059d0_0;
    .scope S_0x11ef06c90;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x11ef059d0_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x11ef06c90;
T_1 ;
    %delay 50, 0;
    %load/vec4 v0x11ef059d0_0;
    %inv;
    %store/vec4 v0x11ef059d0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x11ef1a8a0;
T_2 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x11ef1ad00_0, 0, 2;
    %end;
    .thread T_2;
    .scope S_0x11ef1a8a0;
T_3 ;
    %wait E_0x11ef1abf0;
    %load/vec4 v0x11ef1ac40_0;
    %store/vec4 v0x11ef1ad00_0, 0, 2;
    %jmp T_3;
    .thread T_3;
    .scope S_0x11ef05860;
T_4 ;
    %vpi_call 2 8 "$dumpfile", "dffnSim.vcd" {0 0 0};
    %vpi_call 2 9 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x11ef05860 {0 0 0};
    %vpi_call 2 10 "$monitor", " %b %b %b", v0x11ef1ae30_0, v0x11ef1af10_0, v0x11ef1afa0_0, $stime {0 0 0};
    %vpi_call 2 11 "$display", "ck  i  o      time" {0 0 0};
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x11ef1af10_0, 0, 2;
    %delay 100, 0;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x11ef1af10_0, 0, 2;
    %delay 200, 0;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x11ef1af10_0, 0, 2;
    %delay 100, 0;
    %vpi_call 2 15 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
