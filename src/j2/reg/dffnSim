S_0x11d6073b0 .scope module, "dffnSim" "dffnSim" 2 1;
 .timescale 0 0;
v0x11d61d770_0 .net "ck", 0 0, v0x11d607520_0;  1 drivers
v0x11d61d850_0 .var "i", 0 0;
v0x11d61d8e0_0 .net "o", 0 0, v0x11d61d5d0_0;  1 drivers
S_0x11d608fc0 .scope module, "clk1" "clk" 2 4, 3 1 0, S_0x11d6073b0;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "ck";
v0x11d607520_0 .var "ck", 0 0;
S_0x11d61d2a0 .scope module, "dffn1" "dffn" 2 5, 4 1 0, S_0x11d6073b0;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "Q";
    .port_info 1 /INPUT 1 "D";
    .port_info 2 /INPUT 1 "ck";
v0x11d61d520_0 .net "D", 0 0, v0x11d61d850_0;  1 drivers
v0x11d61d5d0_0 .var "Q", 0 0;
v0x11d61d670_0 .net "ck", 0 0, v0x11d607520_0;  alias, 1 drivers
E_0x11d61d4d0 .event negedge, v0x11d607520_0;
    .scope S_0x11d608fc0;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x11d607520_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x11d608fc0;
T_1 ;
    %delay 50, 0;
    %load/vec4 v0x11d607520_0;
    %inv;
    %store/vec4 v0x11d607520_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x11d61d2a0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x11d61d5d0_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x11d61d2a0;
T_3 ;
    %wait E_0x11d61d4d0;
    %load/vec4 v0x11d61d520_0;
    %store/vec4 v0x11d61d5d0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x11d6073b0;
T_4 ;
    %vpi_call 2 8 "$dumpfile", "dffnSim.vcd" {0 0 0};
    %vpi_call 2 9 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x11d6073b0 {0 0 0};
    %vpi_call 2 10 "$monitor", " %b %b %b", v0x11d61d770_0, v0x11d61d850_0, v0x11d61d8e0_0, $stime {0 0 0};
    %vpi_call 2 11 "$display", "ck i o time" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x11d61d850_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x11d61d850_0, 0, 1;
    %delay 200, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x11d61d850_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 15 "$finish" {0 0 0};
    %end;
    .thread T_4;
