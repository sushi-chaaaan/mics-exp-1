S_0x151804170 .scope module, "dffnSim" "dffnSim" 2 1;
 .timescale 0 0;
v0x151814a70_0 .net "ck", 0 0, v0x1518044d0_0;  1 drivers
v0x151814b50_0 .var "i", 0 0;
v0x151814be0_0 .net "o", 0 0, v0x1518148d0_0;  1 drivers
S_0x1518042f0 .scope module, "clk1" "clk" 2 4, 3 1 0, S_0x151804170;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "ck";
v0x1518044d0_0 .var "ck", 0 0;
S_0x1518145a0 .scope module, "dffn1" "dffn" 2 5, 4 1 0, S_0x151804170;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "Q";
    .port_info 1 /INPUT 1 "D";
    .port_info 2 /INPUT 1 "ck";
v0x151814820_0 .net "D", 0 0, v0x151814b50_0;  1 drivers
v0x1518148d0_0 .var "Q", 0 0;
v0x151814970_0 .net "ck", 0 0, v0x1518044d0_0;  alias, 1 drivers
E_0x1518147d0 .event negedge, v0x1518044d0_0;
    .scope S_0x1518042f0;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1518044d0_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x1518042f0;
T_1 ;
    %delay 50, 0;
    %load/vec4 v0x1518044d0_0;
    %inv;
    %store/vec4 v0x1518044d0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x1518145a0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x1518148d0_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x1518145a0;
T_3 ;
    %wait E_0x1518147d0;
    %load/vec4 v0x151814820_0;
    %store/vec4 v0x1518148d0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x151804170;
T_4 ;
    %vpi_call 2 8 "$dumpfile", "dffnSim.vcd" {0 0 0};
    %vpi_call 2 9 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x151804170 {0 0 0};
    %vpi_call 2 10 "$monitor", " %b %b %b", v0x151814a70_0, v0x151814b50_0, v0x151814be0_0, $stime {0 0 0};
    %vpi_call 2 11 "$display", "ck i o time" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x151814b50_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x151814b50_0, 0, 1;
    %delay 200, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x151814b50_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 15 "$finish" {0 0 0};
    %end;
    .thread T_4;
